  <title value="SXCM_TS: GeneralTimingSpecification (V3 Data Type)"/>
  <status value="active"/>
  <experimental value="false"/>
  <date value="2023-10-04T23:00:58+02:00"/>
  <publisher value="Health Level 7"/>
  <contact>
